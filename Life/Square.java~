public class Square {

    private Square _next;
    private int[] color;
    private int xcor;
    private int ycor;
    private String message;
    
    public Square(){
      
    }

    public Square( Square next ){
	_next = next;
    }

    public Square getNext(){
	return _next;
    }
    
    public Square setNext( Square next){
      _next = next;
      return _next;
    }
    
    public int[] getColor(){
	return color;
    }

    public int getXcor(){
	return xcor;
    }

    public int getYcor(){
	return ycor;
    }

    public String getMessage(){
	return message;
    }

}
